@model CreateUserVM

@{
    ViewBag.Title = "Skapa ny användare";
}

<center>
    @*fixa asp-action!*@
    <form asp-controller="Create" asp-action="Index">
        <label asp-for="UserName"></label>
        <input asp-for="UserName" />
        <span asp-validation-for="UserName"></span>

        <label asp-for="PassWord"></label>
        <input asp-for="PassWord" />
        <span asp-validation-for="PassWord"></span>

        <label asp-for="ChildCode"></label>
        <input asp-for="ChildCode" />
        <span asp-validation-for="ChildCode"></span>

        <br />
        <input type="submit" style="background-color:white" value="Skapa!" />
        
    </form>

    
</center>